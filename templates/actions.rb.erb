class <%= name %>
  class Actions
    attr_accessor :parser, :result
<% rules.map(&:components).flatten.map(&:elements).flatten.grep(Bison::Action).each_with_index do |action, i| %>
    def <%= action.name %>(<%= action.predecessor_tags.values.join(', ') %>)
      <%= action.code.strip %>
    end
<% end -%>
  end
end
